<Page x:Class="tst1.Reg"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:tst1"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Reg">

    <Grid>
        <TextBox x:Name="Login" HorizontalAlignment="Center" Margin="0,181,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Password" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" Width="120"/>
        <Button Content="Зарегестрироватся" HorizontalAlignment="Center" Margin="0,239,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button Content="Выйти" HorizontalAlignment="Center" Margin="0,264,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
    </Grid>
</Page>
